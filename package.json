{
  "name": "plugin-wp-importer",
  "version": "0.1.0",
  "description": "Importer plugin for Wordpress",
  "private": true,
  "scripts": {
    "lint": "eslint 'src/**/*.ts' --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "typecheck": "tsc -p . --noEmit",
    "copyfiles": "cpy './README.md' './LICENSE' './package.json' dist/",
    "compile:tsc": "tsc -p ./tsconfig.production.json",
    "compile": "yarn clean && yarn compile:tsc && yarn copyfiles",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
